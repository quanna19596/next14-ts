{
  "name": "my-next14",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "lint:staged": "lint-staged",
    "prettier": "prettier --check --log-level silent \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "format": "prettier --write --log-level silent \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "next": "14.2.17",
    "next-intl": "^3.25.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "14.2.17",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.59.2",
    "typescript": "^5.4.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn format",
      "yarn lint"
    ],
    "**/*.{tsx,ts}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
